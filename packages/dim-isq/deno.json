{
  "name": "@isentropic/dim-isq",
  "description": "Ready-to-use ISQ quantity system for compile-time dimensional analysis",
  "license": "MIT",
  "keywords": [
    "typescript",
    "dimensional-analysis",
    "isq",
    "quantity-system"
  ],
  "publish": {
    "exclude": [
      "quantities.spec.ts",
      "test.ts"
    ]
  },
  "exports": {
    ".": "./src/mod.ts",
    "./ops": "./src/ops.ts"
  },
  "tasks": {
    "generate:quantities": "deno run -RW @isentropic/dim-quantity/generate --spec ./quantities.spec.ts --out ./src/quantities.generated.ts && deno fmt src/quantities.generated.ts"
  },
  "version": "0.4.3"
}
